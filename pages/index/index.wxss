/**index.wxss**/

.container {
  background: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  position: relative;
}

.container .bgImg {
  width: 100%;
  height: 100%;
}

.container .logo {
  width: 352rpx;
  height: 80rpx;
  position: absolute;
  left: 50%;
  top: 80rpx;
  transform: translateX(-50%);
}

.container .part {
  width: 640rpx;
  height: 580rpx;
  position: absolute;
  top: 200rpx;
  left: 50%;
  transform: translateX(-50%);
}

.container .part .part01 {
  width: 100%;
  height: 160rpx;
  background: #fff;
  border-radius: 5px;
  box-shadow: 0 10px 8px -8px rgba(8, 51, 124, 0.2);
  position: relative;
  margin-top: 20px;
}

.container .part .part01 .part01-img {
  width: 175rpx;
  height: 100%;
  position: absolute;
  left: 0;
}

.container .part .part01 .part01-img image {
  width: 50rpx;
  height: 52rpx;
  display: block;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.container .part .second .part01-img image {
  width: 51rpx;
  height: 42rpx;
}

.container .part .third .part01-img image {
  width: 42rpx;
  height: 52rpx;
}

.container .part .part01 .part01-img text {
  color: #ddd;
  float: right;
  margin-top: 55rpx;
}

.container .part .part01 .part01-img02 {
  width: 460rpx;
  height: 100%;
  position: absolute;
  right: 0;
}

.container .part .part01 .part01-img02 image {
  width: 16rpx;
  height: 30rpx;
  display: block;
  position: absolute;
  right: 8%;
  top: 50%;
  transform: translateY(-50%);
}

.container .part .part01 .part01-img02 text {
  font-size: 36rpx;
  color: #99bfff;
  line-height: 160rpx;
  text-align: center;
  display: block;
  margin-left: -15rpx;
}
.button {
  width: 432rpx;
  height: 130rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 900rpx;
  text-align: center;
}
.button image{
  width: 432rpx;
  height: 98rpx;
 
}
.button text {
  font-size: 24rpx;
  color: #b4b4b4;
}


